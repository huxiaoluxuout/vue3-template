{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^ylx-(.*)": "@/components/ylx-components/ylx-$1.vue",
      "^mf-(.*)": "@/components/mf-components/mf-$1.vue",
      "^test-(.*)": "@/components/test-components/test-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "%home.title%",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/mine/mine",
      "style": {
        "navigationBarTitleText": "%mine.title%",
        "navigationBarBackgroundColor": "#fff",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/deviceManagement/deviceManagement",
      "style": {
        "navigationBarTitleText": "分组管理",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/newManagement/newManagement",
      "style": {
        "navigationBarTitleText": "新增管理",
        "enablePullDownRefresh": false
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#F8F8F8"
  },
  "subPackages": [
    {
      "root": "pagesSub",
      "pages": [
        {
          "path": "webView/webView",
          "style": {
            "navigationBarTitleText": ""
          }
        },
        {
          "path": "richText/richText",
          "style": {
            "navigationBarTitleText": ""
          }
        }
      ]
    },
    {
      "root": "pagesSubMine",
      "pages": [
        {
          "path": "myOrder/myOrder",
          "style": {
            "navigationBarTitleText": "我的订单",
            "enablePullDownRefresh": true
          }
        },
        {
          "path": "orderDetails",
          "style": {
            "navigationBarTitleText": "订单详情",
            "enablePullDownRefresh": true
          }
        }
      ]
    }
  ],
  "tabBar": {
    "custom": false,
    "selectedColor": "#3677F9",
    "color": "#999999",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "text": "设备列表",
        "pagePath": "pages/index/index",
        "iconPath": "static/tabbar/tabbar1.png",
        "selectedIconPath": "static/tabbar/on_tabbar1.png"
      },
      {
        "text": "分组管理",
        "pagePath": "pages/deviceManagement/deviceManagement",
        "iconPath": "static/tabbar/tabbar2.png",
        "selectedIconPath": "static/tabbar/on_tabbar2.png"
      },

       {
        "text": "新增管理",
        "pagePath": "pages/newManagement/newManagement",
        "iconPath": "static/tabbar/tabbar3.png",
        "selectedIconPath": "static/tabbar/on_tabbar3.png"
      },
       {
        "text": "个人中心",
        "pagePath": "pages/mine/mine",
        "iconPath": "static/tabbar/tabbar4.png",
        "selectedIconPath": "static/tabbar/on_tabbar4.png"
      }


    ]
  },
  "uniIdRouter": {}
}
